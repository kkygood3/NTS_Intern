<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nts.reservation.mapper.FileMapper">
	<insert id="insertFile" useGeneratedKeys="true" keyProperty="id">
		INSERT INTO file_info 
		            (file_name, 
		             save_file_name, 
		             content_type, 
		             delete_flag, 
		             create_date, 
		             modify_date) 
		VALUES      (#{fileName}, 
		             #{saveFileName}, 
		             #{contentType}, 
		             #{deleteFlag}, 
		             Now(), 
		             Now()); 
	</insert>
</mapper>